{
  "expo": {
    "name": "Object Translator",
    "slug": "ObjectTranslator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#49454F"
    },
    "updates": { "fallbackToCacheTimeout": 0 },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "config": { "usesNonExemptEncryption": false },
      "supportsTablet": true,
      "bundleIdentifier": "com.objecttranslator",
      "buildNumber": "1.9"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#49454F"
      },
      "package": "com.objecttranslator"
    },
    "web": { "favicon": "./assets/favicon.png" }
  }
}
